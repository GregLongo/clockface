{
  "name": "@influxdata/clockface",
  "version": "0.0.7",
  "license": "MIT",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "description": "UI Kit for building Chronograf and other Time Series visualization applications",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "github:influxdata/clockface"
  },
  "devDependencies": {
    "@css-blocks/core": "^0.19.0",
    "@css-blocks/jsx": "^0.20.0-beta.1",
    "@types/react": "^16.4.14",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "microbundle": "^0.9.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "classnames": "^2.2.6"
  },
  "scripts": {
    "move-fonts": "cp -r src/Styles/Fonts dist/",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && npm run lint && microbundle && npm run move-fonts && rm -rf .rts2_cache_*",
    "dev": "microbundle watch",
    "clean": "rm -rf ./dist && rm -rf ./.cache && rm -rf node_modules",
    "lint": "npm run tsc && npm run eslint && npm run prettier",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "tsc": "tsc -p ./tsconfig.json --noEmit --pretty --skipLibCheck"
  }
}
